    <link rel="stylesheet" type="text/css" href="/css/editor-styles.css">
    <title>Code-20</title>
  </head>
  <body>
    <form class="editor-form w-100" action="/" method="post">
      <div class="content w-100 container-fluid d-flex flex-column flex-wrap">
        <div class="row">
          <div class="col-md">
            <nav class="navbar navbar-expand-sm navbar-dark bg-dark">
              <div class="container-fluid">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                  <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                  <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item dropdown">
                      <a class="nav-link dropdown-toggle" href="#" id="fileDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                        File
                      </a>
                      <ul class="dropdown-menu" aria-labelledby="fileDropdown">
                        <li><a class="dropdown-item" href="#">New</a></li>
                        <li><a class="dropdown-item" href="#">Save</a></li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="dropdown-item" href="#">Open</a></li>
                      </ul>
                    </li>
                    <li class="nav-item dropdown">
                      <a class="nav-link dropdown-toggle" href="#" id="editDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                        Edit
                      </a>
                      <ul class="dropdown-menu" aria-labelledby="editDropdown">
                        <li><a class="dropdown-item" href="#">Undo</a></li>
                        <li><a class="dropdown-item" href="#">Redo</a></li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="dropdown-item" href="#">Cut</a></li>
                        <li><a class="dropdown-item" href="#">Copy</a></li>
                        <li><a class="dropdown-item" href="#">Paste</a></li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="dropdown-item" href="#">Comment/Uncomment</a></li>
                        <li><a class="dropdown-item" href="#">Select All</a></li>
                      </ul>
                    </li>
                    <li class="nav-item dropdown">
                      <a class="nav-link dropdown-toggle" href="#" id="viewDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                        View
                      </a>
                      <ul class="dropdown-menu" aria-labelledby="viewDropdown">
                        <li><a class="dropdown-item" href="#">Split Left</a></li>
                        <li><a class="dropdown-item" href="#">Split Right</a></li>
                        <li><a class="dropdown-item" href="#">Split Up</a></li>
                        <li><a class="dropdown-item" href="#">Split Down</a></li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="dropdown-item" href="#">Show/Hide Navigator</a></li>
                        <li><a class="dropdown-item" href="#">Show/Hide IO</a></li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="dropdown-item" href="#">Increase Font Size</a></li>
                        <li><a class="dropdown-item" href="#">Decrease Font Size</a></li>
                      </ul>
                    </li>
                    <li class="nav-item dropdown">
                      <a class="nav-link dropdown-toggle" href="#" id="runDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                        Run
                      </a>
                      <ul class="dropdown-menu" aria-labelledby="runDropdown">
                        <li><a class="dropdown-item" href="#">Compile [filename.ext]</a></li>
                        <li><a class="dropdown-item" href="#">Run [filename]</a></li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="dropdown-item" href="#">Stress [filename]</a></li>
                        <li><a class="dropdown-item" href="#">Interact [filename]</a></li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="dropdown-item" href="#">Show/Hide Console</a></li>
                      </ul>
                    </li>
                  </ul>
                </div>
              </div>
            </nav>
          </div>
        </div>

        <!-- <div class="code"></div> -->

          <div class="row d-flex flex-grow-1 flex-shrink-1"> <!-- class="editor-container" -->
            <div class="col-md-6 p-0">
              <input type="hidden" name="codeMain" class="editor-placeholder" id="code-main-placeholder"></textarea>
              <div class="editor-container" id="code-main-container"></div>
            </div>
            <div class="col p-0 d-flex flex-column flex-wrap">
              <div class="row d-flex flex-grow-1 flex-shrink-1">
                <div class="col-md d-flex">
                  <input type="hidden" name="inputMain" class="editor-placeholder" id="input-main-placeholder"></textarea>
                  <div class="editor-container" id="input-main-container"></div>
                </div>
              </div>
              <div class="row d-flex flex-grow-1 flex-shrink-1">
                <div class="col-md d-flex flex-column">
                  <input type="hidden" name="outputMain" class="editor-placeholder" id="output-main-placeholder"></textarea>
                  <div class="editor-container" id="output-main-container"></div>
                </div>
              </div>
            </div>
          </div>

          <div class="btn-group commands w-50 align-items-end" role="group" aria-label="Commands">
            <button type="button" name="cmd" value="stress" class="mw-10 btn btn-primary cmd cmd-stress">Stress</button>
            <button type="button" name="cmd" value="interact" class="mw-10 btn btn-primary cmd cmd-interact">Interact</button>
            <button type="button" name="cmd" value="compile" class="mw-10 btn btn-primary cmd cmd-compile">Compile</button>
            <button type="button" name="cmd" value="run" class="mw-10 btn btn-primary cmd cmd-run">Run</button>
            <button type="submit" name="submit" id="submit-btn" class="submit-btn"></button>
          </div>
      </div>
    </form>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
